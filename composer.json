{
    "name": "szymsza/laravel-fakturoid",
    "description": "Fakturoid Laravel Wrapper",
    "keywords": [
        "webiz",
        "laravel",
        "ecommerce",
        "invoicing"
    ],
    "homepage": "https://github.com/szymsza/laravel-fakturoid",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Jakub Szymsza",
            "email": "jakub@szymsza.cz",
            "role": "Developer"
        },
        {
            "name": "Dominik Nguyen",
            "email": "dom@webiz.cz",
            "role": "Developer"
        },
        {
            "name": "Jakub Theimer",
            "email": "theimer@madne.st",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "composer require szymsza/fakturoid-php-8.0": "dev-main",
        "guzzlehttp/guzzle": "^7.8",
        "illuminate/support": ">=6.0",
        "laravel/framework": ">=6.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.3",
        "orchestra/testbench": "^8.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "WEBIZ\\LaravelFakturoid\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "WEBIZ\\LaravelFakturoid\\Tests\\": "tests"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "WEBIZ\\LaravelFakturoid\\FakturoidServiceProvider"
            ],
            "aliases": {
                "Fakturoid": "WEBIZ\\LaravelFakturoid\\Facade"
            }
        }
    }
}
